#include <iostream>
#include<stack>
using namespace std;

void insertAtBottom(stack<int>& s){
  if(s.empty()){
    cout<<"Stack is empty()"<<endl;
    return;
  }
  int target = s.top();
  s.pop();
  insertAtBottomHelper(s,target);
}
void insertAtBottomHelper(stack<int> &s, int &target){
  if(s.empty()){
    s.push(target);
    return ;
  }

  int temp = s.top();
  s.pop();
  insertAtBottomHelper(s,target);
  s.push(temp);
}
int main() {
  stack<int> s;

  s.push(1);
  s.push(2);
  s.push(3);
  s.push(4);
  s.push(5);
  s.push(6);
  s.push(7);

  


  insertAtBottom(s);

  while(!s.empty()){
    cout<<s.top()<<" ";
    s.pop();
  }

  

  return 0;
}

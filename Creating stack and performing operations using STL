#include <iostream>
#include<stack>
using namespace std;

int main() {
  
  // creation
  stack<int> s;

  // push
  s.push(10);
  s.push(20);
  s.push(30);
  s.push(40);
  s.push(50);
  cout<<"Size of Stack :"<<s.size()<<endl;

  // pop
  s.pop();
  s.pop();

  // size

  cout<<"Size of Stack :"<<s.size()<<endl;

  // check empty
  cout<<"isEmpty :"<<s.empty()<<endl;

  // top
  cout<<"element at top of Stack :"<<s.top()<<endl;

  // print

  cout<<"Printing Stack :";
  while(!s.empty()){
    cout<<s.top()<<" ";
    s.pop();
  }
  cout<<endl;

  return 0;
}

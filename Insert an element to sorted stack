#include <iostream>
#include<stack>
using namespace std;


void insertSorted(stack<int> & s,int val){
  if(s.empty()){
    s.push(val);
    return;
  }

  if(val >= s.top()){
    s.push(val);
    return;
  }
  int topElement = s.top();
  s.pop();
  insertSorted(s,val);
  s.push(topElement);
}


int main() {
  stack<int> s;

  s.push(10);
  s.push(20);
  s.push(30);
  s.push(40);
  s.push(50);
  s.push(60);

  
  insertSorted(s,15);

  cout<<"Printing Result: "<<endl;
  while(!s.empty()){
    cout<<s.top()<<" ";
    s.pop();
  }
  cout<<endl;

  return 0;
}

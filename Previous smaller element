class Solution {
  public:
    vector<int> leftSmaller(vector<int> arr) {
        vector<int> pse(arr.size());
        
        stack<int> s;
        s.push(-1);
        for(int i=0;i<arr.size();i++){
            while(s.top() >= arr[i]){
                s.pop();
            }
            pse[i] = s.top();
            s.push(arr[i]);
        }
        
        return pse;
        
    }
};
